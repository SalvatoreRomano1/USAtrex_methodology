{
   "author": "The team of https://tracking.exposed",
   "background": {
      "persistent": false,
      "scripts": [ "./background.js" ]
   },
   "browser_action": {
      "default_icon": "yttrex16.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "./app.js" ],
      "matches": [ "https://*.tracking.exposed/*", "https://*.youtube.com/*" ]
   } ],
   "description": "A tool for academic researchers. Part of the tracking.exposed project, is meant to analyze the algorithm of a popular video portal.",
   "icons": {
      "128": "yttrex128.png",
      "16": "yttrex16.png",
      "48": "yttrex48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ytTREX",
   "permissions": [ "storage", "cookies", "tabs", "https://*.youtube.com/", "https://*.tracking.exposed/" ],
   "short_name": "ytTREX",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.2",
   "web_accessible_resources": [ "*.png" ]
}
